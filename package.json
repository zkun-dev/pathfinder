{
  "name": "pathfinder",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev:client": "vite --host --port 3000",
    "dev": "pnpm dev:client",
    "build:client": "vite build --outDir dist/static",
    "build": "rm -rf dist && pnpm build:client && cp package.json dist && touch dist/build.flag",
    "typecheck": "vue-tsc --noEmit",
    "lint": "echo 'No linter configured. Consider adding ESLint for code quality checks.'"
  },
  "dependencies": {
    "@vueuse/core": "^11.3.0",
    "@vueuse/motion": "^2.0.0",
    "tailwind-merge": "^3.0.2",
    "three": "^0.160.0",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/three": "^0.160.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.1",
    "typescript": "~5.7.2",
    "update-browserslist-db": "^1.2.3",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vue-tsc": "^2.1.10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "vue-demi"
    ]
  }
}
